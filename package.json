{
  "name": "capstone",
  "version": "0.1.0",
  "private": true,
  "author": "Heath Pack",
  "description": "App",
  "devDependencies": {
    "electron": "^4.0.1",
    "electron-builder": "^20.38.4"
  },
  "dependencies": {
    "asar": "^0.14.6",
    "concurrently": "^4.1.0",
    "electron-is-dev": "^1.0.1",
    "jquery": "^3.3.1",
    "knex": "^0.16.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "sqlite": "^3.0.0",
    "wait-on": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \" SET BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder  -c.extraMetadata.main=build/main.js",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "public/main.js",
  "homepage": "./",
  "build": {
    "extraResources": [
      "./extraResources/**"
    ]
  }
